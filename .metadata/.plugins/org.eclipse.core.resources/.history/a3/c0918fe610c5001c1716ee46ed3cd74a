
public class Wand {
	private String name;
	private double power;

	public void setName(String name) {
		this.name=name;
	}
	public void setPower(double power) {
		this.power=power;
	}
	public String getName(String name) {
		return this.name;
	}
	public double getPower(double power) {
		return this.power;
	}
}
