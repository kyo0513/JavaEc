
public class HeroApp {
	public static void main(String[] args) {
		Hero h = new Hero();
		h.setName("テスト");
		h.setHp(100);
		Wand wand=new Wand();
		wand.setName("杖");
		wand.setPower(50);
		Wizard wiz = new Wizard();
		wiz.setHp(100);
		wiz.setMp(100);
		wiz.setName("WIZ");
		wiz.setWand(wand);
		wiz.heal(h);
	}
}
